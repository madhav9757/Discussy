{
  "version": 2,
  "name": "your-frontend-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_API_BASE_URL": "https://your-backend-app-name.vercel.app/api"    
  }
}