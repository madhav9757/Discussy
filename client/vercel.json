{
  "version": 2,
  "name": "discussly-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV":"production",
    "VITE_API_BASE_URL": "https://discussy.onrender.com"
  }
}
